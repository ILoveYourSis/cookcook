package com.likefood.cookcook;

import android.content.Context;
import android.widget.Button;

public class MenuItem extends Button 
{
    public MenuItem(Context ct)
    {
    	super(ct);
    }
    
    private String _id;
    public String getId()
    {
    	return _id;
    }
    
    public void setId(String id)
    {
    	_id = id;
    }
}
