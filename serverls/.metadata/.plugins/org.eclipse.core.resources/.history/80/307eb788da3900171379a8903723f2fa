package CookTable;

public abstract class TableBase
{
	/*
	 * 从数据库中读取所有，这个是初始化用的
	 */
	public void readFromDb(){}
	
	/*
	 * 全部存到数据库中，这个是数据库管理类定期调用的
	 */
	public void saveAll(){}
	
	/*
	 * 更新一条数据
	 */
	public void updateRecord(RecordBase record){}
	
	
	public void insertRecord(RecordBase record){}

}
